.suggesterContainer
  position absolute
  bottom 20px
  left 30px
  border-radius 4px
  border 2px solid lighten(black, 80%)
  padding 6px 10px
  height 74px
  background #FFF

.subsetSelector-container
  padding 5px
  width 240px
  input
  ul.suggestionList
    border 1px solid #aaa
    border-radius 3px
    width @width
  ul.suggestionList
    &:empty
      display none
    position absolute
    bottom 38px
    border-bottom 0px none
    left 8px
    list-style-type none
    margin 0px
    padding 0px
    background white
    li
      padding 8px 18px
      position relative
      .okres
        color #666
      &.active
        background ColorHeadingLink
        color white
        cursor pointer
        .okres
          color #eee


  input
    margin 0px
    padding 5px 10px
    height 30px
    position absolute
    left 8px
    bottom 8px

